<p id="notice"><%= notice %></p>

<h1>Listing Programacions</h1>

<table>
  <thead>
    <tr>
      <th>Iduser</th>
      <th>Idemisora</th>
      <th>Dia</th>
      <th>Hora</th>
      <th>Titulo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @programacions.each do |programacion| %>
      <tr>
        <td><%= programacion.idUser %></td>
        <td><%= programacion.idEmisora %></td>
        <td><%= programacion.dia %></td>
        <td><%= programacion.hora %></td>
        <td><%= programacion.titulo %></td>
        <td><%= link_to 'Show', programacion %></td>
        <td><%= link_to 'Edit', edit_programacion_path(programacion) %></td>
        <td><%= link_to 'Destroy', programacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Programacion', new_programacion_path %>
